{
    "calva.fmt.allgnMapItems": true,
    "calva.fmt.newIndentEngine": true,
    "workbench.startupEditor": "newUntitledFile",
    "editor.minimap.enabled": true,
    "breadcrumbs.enabled": true,
    "files.autoSave": "onFocusChange",
    "editor.dragAndDrop": false,
    "editor.emptySelectionClipboard": false,
    "editor.formatOnPaste": true,
    "editor.acceptSuggestionOnEnter": "off",
    "search.exclude": {
        "*.log": true
    },
    "terminal.integrated.confirmOnExit": true,
    "calva.autoConnect": false,
    "workbench.activityBar.visible": false,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "constant",
                    "constant.keyword"
                ],
                "settings": {
                    "foreground": "#b8ad1a"
                },
            },
            {
                "scope": [
                    "constant.keyword.clojure",
                    "constant.keyword.fennel"
                ],
                "settings": {
                    "foreground": "#9cdcfeff"
                }
            }
        ]
    },
    "workbench.colorTheme": "Cyberpunk",
    "editor.renderControlCharacters": false,
    "editor.wordWrap": "on",
    "editor.copyWithSyntaxHighlighting": false,
    "editor.matchBrackets": "never",
    "calva.syncReplNamespaceToCurrentFile": true,
    "calva.openBrowserWhenFigwheelStarted": false,
    "calva.fmt.removeConsecutiveBlankLines": true,
    "clojureVSCode.autoStartNRepl": false,
    "editor.fontSize": 14,
    "clojureVSCode.formatOnSave": false
}
